{
  "Seed": 42,

  "Platform": "Knative",
  "YAMLSelector": "kwok",
  "EndpointPort": 80,

  "TracePath": "/users/hancheng/traces/sampled_150/$FUNC",
  "Granularity": "minute",
  "OutputPathPrefix": "data/out/$EXPERIMENT/experiment",
  "IATDistribution": "exponential_shift",
  "CPULimit": "1vCPU",
  "ExperimentDuration": 40,
  "WarmupDuration": 20,

  "IsPartiallyPanic": true,
  "EnableZipkinTracing": false,
  "EnableMetricsScrapping": true,
  "MetricScrapingPeriodSeconds": 15,
  "AutoscalingMetric": "concurrency",

  "GRPCConnectionTimeoutSeconds": 15,
  "GRPCFunctionTimeoutSeconds": 900
}
