{
  "Seed": 42,

  "Platform": "nexus",
  "InvokeProtocol" : "grpc",
  "YAMLSelector": "nexus",
  "EndpointPort": 80,

  "BusyLoopOnSandboxStartup": false,

  "TracePath": "data/traces/nexus",
  "Granularity": "minute",
  "OutputPathPrefix": "data/out/$EXPERIMENT/experiment",
  "IATDistribution": "exponential",
  "CPULimit": "1vCPU",
  "ExperimentDuration": $EXP_DUR,
  "WarmupDuration": $WARMUP,

  "IsPartiallyPanic": false,
  "EnableZipkinTracing": false,
  "EnableMetricsScrapping": true,
  "MetricScrapingPeriodSeconds": 15,
  "AutoscalingMetric": "concurrency",

  "GRPCConnectionTimeoutSeconds": 60,
  "GRPCFunctionTimeoutSeconds": 900,
  "DAGMode": false,
  "EnableDAGDataset": true,
  "Width": 2,
  "Depth": 2
}
