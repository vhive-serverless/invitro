{
  "Seed": 42,

  "Platform": "URL",
  "InvokeProtocol" : "grpc",
  "YAMLSelector": "container",
  "EndpointPort": 80,

  "BusyLoopOnSandboxStartup": false,

  "RpsTarget": $RPS_TARGET,
  "RpsColdStartRatioPercentage": 0,
  "RpsCooldownSeconds": 10,
  "RpsFunctionCount": $NUM_SERVERS,
  "RpsImage": "ghcr.io/vhive-serverless/invitro_empty_function:latest",
  "RpsRuntimeMs": 10,
  "RpsMemoryMB": 2048,
  "RpsIterationMultiplier": 80,

  "TracePath": "RPS",
  "Granularity": "second",
  "OutputPathPrefix": "invitro/data/out/$EXPERIMENT/experiment",
  "IATDistribution": "exponential",
  "CPULimit": "1vCPU",
  "ExperimentDuration": $EXPERIMENT_DURATION,
  "WarmupDuration": $WARMUP_DURATION,

  "IsPartiallyPanic": false,
  "EnableZipkinTracing": false,
  "EnableMetricsScrapping": false,
  "MetricScrapingPeriodSeconds": 15,
  "AutoscalingMetric": "concurrency",

  "GRPCConnectionTimeoutSeconds": 15,
  "GRPCFunctionTimeoutSeconds": 20
}